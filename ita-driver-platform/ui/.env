# Island Traffic Authority Driver Platform - Frontend Environment

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_API_TIMEOUT=10000

# Application Configuration
VITE_APP_NAME="Island Traffic Authority"
VITE_APP_VERSION="1.0.0"
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_ONLINE_PAYMENTS=true
VITE_ENABLE_SMS_NOTIFICATIONS=false
VITE_ENABLE_EMAIL_NOTIFICATIONS=true
VITE_ENABLE_DARK_MODE=true

# Island-Specific Configuration  
VITE_DEFAULT_TIMEZONE="Atlantic/Bermuda"
VITE_DEFAULT_LANGUAGE=en
VITE_CURRENCY="BMD"
VITE_PHONE_FORMAT="+1 (441) XXX-XXXX"

# Test Centers
VITE_TEST_CENTERS="Hamilton,St. George,Somerset"

# External Services
VITE_GOOGLE_MAPS_API_KEY=""
VITE_STRIPE_PUBLISHABLE_KEY=""

# Monitoring
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=""

# Development
VITE_ENABLE_DEVTOOLS=true
VITE_LOG_LEVEL=debug